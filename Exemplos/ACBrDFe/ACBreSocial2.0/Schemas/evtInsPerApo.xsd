                                          </xs:restriction>
                                        </xs:simpleType>
                                      </xs:element>
                                      <xs:element name="tecMedicao" minOccurs="0">
                                        <xs:simpleType>
                                          <xs:annotation>
                                            <xs:documentation>Técnica utilizada para medição</xs:documentation>
                                          </xs:annotation>
                                          <xs:restriction base="xs:string">
